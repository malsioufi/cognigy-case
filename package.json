{
  "name": "cognigy-case",
  "version": "1.0.0",
  "description": "Technical test for backend developer role at Cognigy.",
  "main": "dist/server.js",
  "engines": {
    "node": "16.13.0"
  },
  "scripts": {
    "start": "node dist/server.js",
    "watch": "ts-node-dev --rs --respawn src/server.ts",
    "build": "npm run lint && npm test && npm run build:ts",
    "build:ts": "tsc",
    "lint": "eslint src --ext .js,.ts --ignore-path .gitignore",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": "Mohamad Alsioufi",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.7",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.3.1",
    "mongodb-memory-server": "8.0.2",
    "prettier": "2.4.1",
    "supertest": "6.1.6",
    "ts-jest": "27.0.7",
    "ts-node-dev": "1.1.8",
    "typescript": "4.4.4"
  },
  "dependencies": {
    "ajv": "8.8.1",
    "express": "4.17.1",
    "mongoose": "6.0.13"
  }
}
